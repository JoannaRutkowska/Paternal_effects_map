@ARTICLE{Pacchierotti2015,
author={Pacchierotti, F. and Spanò, M.},
title={Environmental Impact on DNA Methylation in the Germline: State of the Art and Gaps of Knowledge},
journal={BioMed Research International},
year={2015},
volume={2015},
doi={10.1155/2015/123484},
art_number={123484},
note={cited By 31},
url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-84939839770&doi=10.1155%2f2015%2f123484&partnerID=40&md5=ae70dc26c7ea6216c87652e68cbac7ba},
