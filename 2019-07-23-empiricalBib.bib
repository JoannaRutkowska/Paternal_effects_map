@ARTICLE{González-Rojo2019368,
author={González-Rojo, S. and Lombó, M. and Fernández-Díez, C. and Herráez, M.P.},
title={Male exposure to bisphenol a impairs spermatogenesis and triggers histone hyperacetylation in zebrafish testes},
journal={Environmental Pollution},
