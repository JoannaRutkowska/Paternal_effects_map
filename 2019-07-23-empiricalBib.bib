@ARTICLE{Killinger2012902,
author={Killinger, C.E. and Robinson, S. and Stanwood, G.D.},
title={Subtle biobehavioral effects produced by paternal cocaine exposure},
journal={Synapse},
year={2012},
volume={66},
number={10},
pages={902-908},
doi={10.1002/syn.21582},
note={cited By 23},
url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-84864588306&doi=10.1002%2fsyn.21582&partnerID=40&md5=fad40c56fe3fdf98853fa40025c91532},
affiliation={Department of Pharmacology, Vanderbilt University School of Medicine, Nashville, TN 37232, United States; Vanderbilt Kennedy Center for Research on Human Development, Vanderbilt University School of Medicine, Nashville, TN 37232, United States; Department of Physiology and Pharmacology, Wake Forest School of Medicine, Winston-Salem, NC, United States},
abstract={Despite the increased prevalence of cocaine use and abuse in males when compared with females, possible effects of paternal cocaine exposure on biobehavioral development have received little attention. We therefore exposed male mice to cocaine (20 mg/kg, i.p.) or vehicle for 10 weeks and then used those mice as sires. We then behaviorally phenotyped the F1 offspring to assess the consequences of paternal cocaine exposure on brain function. We report the presence of a subtle but significant increase in immobility in the tail suspension test, a measure of behavioral depression, following paternal cocaine. Body weight was also significantly decreased in paternal cocaine-exposed offspring. Other aspects of neurobehavioral function, including locomotor activity, anxiety, and learning and memory, were not affected by paternal cocaine history. These data suggest alterations in brain systems and/or circuitry underlying mood regulation in the offspring of cocaine-using fathers. Synapse 2012. © 2012 Wiley Periodicals, Inc.},
