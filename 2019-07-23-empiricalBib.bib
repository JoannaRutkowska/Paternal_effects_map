@ARTICLE{Bonduriansky20161675,
author={Bonduriansky, R. and Runagall-McNaull, A. and Crean, A.J.},
title={The nutritional geometry of parental effects: maternal and paternal macronutrient consumption and offspring phenotype in a neriid fly},
journal={Functional Ecology},
year={2016},
