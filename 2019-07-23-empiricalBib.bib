@ARTICLE{Bieber2006189,
author={Bieber, A.M. and Marcon, L. and Hales, B.F. and Robaire, B.},
title={Effects of chemotherapeutic agents for testicular cancer on the male rat reproductive system, spermatozoa, and fertility},
