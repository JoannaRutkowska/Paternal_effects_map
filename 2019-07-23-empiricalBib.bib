@ARTICLE{Knezovich2012,
author={Knezovich, J.G. and Ramsay, M.},
title={The effect of preconception paternal alcohol exposure on epigenetic remodeling of the H19 and Rasgrf1 imprinting control regions in mouse offspring},
journal={Frontiers in Genetics},
year={2012},
volume={3},
number={FEB},
doi={10.3389/fgene.2012.00010},
art_number={Article 10},
note={cited By 61},
url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-84870567826&doi=10.3389%2ffgene.2012.00010&partnerID=40&md5=80d360fd1abdc910dc2e52c34554db53},
affiliation={Molecular Genetics Laboratory, Division of Human Genetics, University of the Witwatersrand, Johannesburg, South Africa},
abstract={Imprinted loci play a critical role in fetal development. Their expression is often regulated by CCCTC-binding factor (CTCF) protein binding at imprinting control regions (ICRs). Prenatal alcohol exposure has been shown to reduce global DNA methylation in the developing mouse fetus. This study explored the effect of preconception paternal alcohol exposure on DNA methylation at two paternally methylated ICRs (H19 and Rasgrf1) in the sperm of exposed males and somatic DNA of sired offspring. Significant reductions at the H19 CTCF 1 (p= 0.0027) and CTCF 2 (p= 0.0009) binding sites were observed in the offspring of ethanol-treated sires, which was significantly correlated with reduced weight at postnatal days 35-42 (p< 0.05). As birth weight was unaffected and growth was only delayed during the postnatal weaning period, with subsequent re-convergence, we hypothesize that this may be the result of a mental deficit causing delayed establishment of independent feeding following weaning and would explain why this effect is transient. No difference in DNA methylation was observed in the sperm of alcohol-exposed males, indicating that the transmission of the epigenetic signal at conception is not due to altered methylation, but may be the result of an RNA-mediated mechanism or altered chromatin remodeling. © 2012 Knezovich and Ramsay.},
author_keywords={CTCF binding site;  Epigenetics;  H19;  Imprinting control region;  Preconception paternal alcohol exposure;  Rasgrf1},
