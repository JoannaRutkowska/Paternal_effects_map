@ARTICLE{Smith2014,
author={Smith, R.G. and Fernandes, C. and Kember, R. and Schalkwyk, L.C. and Buxbaum, J. and Reichenberg, A. and Mill, J.},
title={Transcriptomic changes in the frontal cortex associated with paternal age},
journal={Molecular Autism},
year={2014},
