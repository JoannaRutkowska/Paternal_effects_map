author={Yang, F. and Yuan, W. and Liang, H. and Song, X. and Yu, Y. and Gelaye, B. and Miao, M. and Li, J.},
title={Preconceptional paternal antiepileptic drugs use and risk of congenital anomalies in offspring: a nationwide cohort study},
journal={European Journal of Epidemiology},
year={2019},
